<TableView class="oracle.dbtools.crest.model.design.relational.TableView" directorySegmentName="seg_0" id="6CE93ADF-F68A-BE98-0534-84DE4E0E0D70" name="vv_std_score">
<createdBy>leads</createdBy>
<createdTime>2021-08-12 09:55:26 UTC</createdTime>
<ownerDesignName>school_management_datamodel</ownerDesignName>
<ordsAlias>view_3</ordsAlias>
<userDefined>true</userDefined>
<userDefinedSQL><![CDATA[SELECT
        wd.school_code,
        wd.batch_no,
        wd.class_code,
        wd.section_code AS sec,
        wd.section_code,
        wd.term_code,
        wd.student_id,
        student_name,
        part_name,
        academic,
        academic_code,
        category_code,
        domain,
        (
            CASE
                WHEN domain IS NOT NULL THEN
                    nvl2(subject, subject, 'Aggregate (' || super_subject || ')')
                WHEN domain IS NULL THEN
                    'Grand Aggregate'
            END
        ) AS subject,
        nvl(main, '-ab-') AS main,
        (
            CASE
                WHEN examination_type = 'INTERNAL' THEN
                    nvl(internal, '-ab-')
                WHEN examination_type IS NULL THEN
                    nvl(internal, '-na-')
            END
        ) AS internal,
        nvl(total, '-ab-') AS total,
        maximum,
        (
            CASE
                WHEN status = 'FAILED' THEN
                    nvl((
                        SELECT
                            ovf.name
                        FROM
                            overall_failed ovf
                        WHERE
                                ovf.status_name = 'ACTIVE'
                            AND ovf.school_code = wd.school_code
                            AND ovf.batch_no# = wd.batch_no
                            AND ovf.class_code = wd.class_code
                            AND ovf.term_code = wd.term_code
                    ), 'FAILED')
                ELSE
                    status
            END
        ) AS status,
        percentage
    FROM
        (
            SELECT
                (
                    SELECT
                        pseudo
                    FROM
                        school
                    WHERE
                        code = school_code
                ) AS school,
                school_code,
                batch_no,
                class_code,
                (
                    SELECT
                        pseudo
                    FROM
                        class
                    WHERE
                        code = class_code
                ) AS class,
                section_code,
                term_code,
                student_id,
                (
                    SELECT
                        first_name || nvl2(middle_name, ' ' || middle_name || ' ', ' ') || last_name
                    FROM
                        student
                    WHERE
                        id = student_id
                ) AS student_name,
                domain_code,
                ranking_type,
                part_name,
                academic_code,
                (
                    SELECT
                        pseudo
                    FROM
                        academic_category
                    WHERE
                        code = academic_code
                ) AS academic,
                category_code,
                (
                    SELECT
                        pseudo
                    FROM
                        subject_category
                    WHERE
                        code = category_code
                ) AS domain,
                super_code,
                (
                    SELECT
                        pseudo
                    FROM
                        super_subject
                    WHERE
                        code = super_code
                ) AS super_subject,
                subject_code,
                (
                    SELECT
                        pseudo
                    FROM
                        subject
                    WHERE
                        code = subject_code
                ) AS subject,
                (
                    CASE
                        WHEN score_type = 'GRADE' THEN
                            (
                                SELECT
                                    b.abbrv
                                FROM
                                         school_grade a
                                    INNER JOIN grade_score b ON ( b.name = a.grade_name )
                                WHERE
                                        a.status_name = 'ACTIVE'
                                    AND a.school_code = school_code
                                    AND round(inter_score / inter_maximum * 100) BETWEEN a.range_from AND a.range_upto
                            )
                        ELSE
                            to_char(round(inter_score))
                    END
                ) AS internal,
                (
                    CASE
                        WHEN score_type = 'GRADE' THEN
                            (
                                SELECT
                                    b.abbrv
                                FROM
                                         school_grade a
                                    INNER JOIN grade_score b ON ( b.name = a.grade_name )
                                WHERE
                                        a.status_name = 'ACTIVE'
                                    AND a.school_code = school_code
                                    AND round(main_score / main_maximum * 100) BETWEEN a.range_from AND a.range_upto
                            )
                        ELSE
                            to_char(round(main_score))
                    END
                ) AS main,
                (
                    CASE
                        WHEN score_type = 'GRADE' THEN
                            (
                                SELECT
                                    b.abbrv
                                FROM
                                         school_grade a
                                    INNER JOIN grade_score b ON ( b.name = a.grade_name )
                                WHERE
                                        a.status_name = 'ACTIVE'
                                    AND a.school_code = school_code
                                    AND round(total_score / total_maximum * 100) BETWEEN a.range_from AND a.range_upto
                            )
                        ELSE
                            to_char(round(total_score))
                    END
                ) AS total,
                round(total_maximum) AS maximum,
                (
                    CASE
                        WHEN ranking_type = 'YES' THEN
                            (
                                CASE
                                    WHEN overall_status = 'PASSED'
                                         AND grp_academic_code = 0
                                         AND grp_category_code = 1
                                         AND grp_domain_code = 1
                                         AND grp_score_type = 0
                                         AND grp_optional_type = 1
                                         AND grp_ranking_type = 0
                                         AND grp_part_name = 0
                                         AND grp_super_code = 1
                                         AND grp_subject_code = 1 THEN
                                        (
                                            SELECT
                                                division_name
                                            FROM
                                                school_division a
                                            WHERE
                                                    a.school_code = wd.school_code
                                                AND a.class_code = wd.class_code
                                                AND round(total_score / total_maximum * 100) BETWEEN a.range_from AND a.range_upto
                                        )
                                    WHEN overall_status = 'FAILED'
                                         AND grp_academic_code = 0
                                         AND grp_category_code = 1
                                         AND grp_domain_code = 1
                                         AND grp_score_type = 0
                                         AND grp_optional_type = 1
                                         AND grp_ranking_type = 0
                                         AND grp_part_name = 0
                                         AND grp_super_code = 1
                                         AND grp_subject_code = 1 THEN
                                        'FAILED'
                                    ELSE
                                        (
                                            CASE
                                                WHEN round(nvl(main_score, 0)) >= round(main_minimum)
                                                     AND round(nvl(total_score, 0)) >= round(total_passed) THEN
                                                    'PASSED'
                                                WHEN round(nvl(main_score, 0)) < round(main_minimum)
                                                     OR round(nvl(total_score, 0)) < round(total_passed) THEN
                                                    'FAILED'
                                            END
                                        )
                                END
                            )
                    END
                ) AS status,
                nvl(to_char(round(total_score / total_maximum * 100)), '-') AS percentage
            FROM
                (
                    SELECT
                        school_code,
                        batch_no,
                        class_code,
                        section_code,
                        term_code,
                        student_id,
                        overall_status,
                        part_name,
                        ranking_type,
                        academic_code,
                        category_code,
                        domain_code,
                        super_code,
                        subject_code,
                        score_type,
                        optional_type,
                        SUM(score) AS score,
                        SUM(passed) AS passed,
                        SUM(minimum) AS min_mark,
                        SUM(maximum) AS max_mark,
                        GROUPING(part_name) AS grp_part_name,
                        GROUPING(ranking_type) AS grp_ranking_type,
                        GROUPING(academic_code) AS grp_academic_code,
                        GROUPING(category_code) AS grp_category_code,
                        GROUPING(domain_code) AS grp_domain_code,
                        GROUPING(super_code) AS grp_super_code,
                        GROUPING(subject_code) AS grp_subject_code,
                        GROUPING(score_type) AS grp_score_type,
                        GROUPING(optional_type) AS grp_optional_type,
                        CASE
                            WHEN GROUPING(examination_type) = 1 THEN
                                'TOTAL'
                            ELSE
                                examination_type
                        END AS examination_type
                    FROM
                        mv_std_score
                    GROUP BY
                        school_code,
                        batch_no,
                        class_code,
                        section_code,
                        term_code,
                        student_id,
                        overall_status,
                        CUBE(part_name,
                             ranking_type,
                             academic_code,
                             category_code,
                             domain_code,
                             super_code,
                             subject_code,
                             score_type,
                             optional_type,
                             examination_type)
                ) PIVOT (
                    SUM ( score )
                AS score, SUM ( passed ) AS passed, SUM ( min_mark ) AS minimum, SUM ( max_mark ) AS maximum
                    FOR examination_type
                    IN ( 'EXTERNAL' AS main, 'INTERNAL' AS inter, 'TOTAL' AS total )
                )
                wd
            WHERE
                (
                    CASE
                        WHEN grp_academic_code = 0
                             AND grp_category_code = 0
                             AND grp_domain_code = 0
                             AND grp_score_type = 0
                             AND grp_optional_type = 0
                             AND grp_ranking_type = 0
                             AND grp_part_name = 0
                             AND grp_super_code = 0
                             AND grp_subject_code = 0 THEN
                            1
                        WHEN ranking_type = 'YES'
                             AND grp_academic_code = 0
                             AND grp_category_code = 1
                             AND grp_domain_code = 1
                             AND grp_score_type = 0
                             AND grp_optional_type = 1
                             AND grp_ranking_type = 0
                             AND grp_part_name = 0
                             AND grp_super_code = 1
                             AND grp_subject_code = 1 THEN
                            1
                        WHEN ranking_type = 'YES'
                             AND grp_academic_code = 0
                             AND grp_category_code = 0
                             AND grp_domain_code = 0
                             AND grp_score_type = 0
                             AND grp_optional_type = 0
                             AND grp_ranking_type = 0
                             AND grp_part_name = 0
                             AND grp_super_code = 0
                             AND grp_subject_code = 1
                             AND EXISTS (
                            SELECT
                                ssubj.super_code
                            FROM
                                     school_subject ssubj
                                INNER JOIN class_subject csubj ON ( csubj.school_code = ssubj.school_code
                                                                    AND csubj.batch_no# = ssubj.batch_no#
                                                                    AND csubj.class_code = ssubj.class_code
                                                                    AND csubj.subject_code = ssubj.subject_code )
                            WHERE
                                    ssubj.status_name = 'ACTIVE'
                                AND csubj.status_name = 'ACTIVE'
                                AND ssubj.school_code = wd.school_code
                                AND ssubj.class_code = wd.class_code
                                AND csubj.batch_no# = wd.batch_no
                                AND csubj.term_code = wd.term_code
                                AND ssubj.super_code = wd.super_code
                            GROUP BY
                                ssubj.super_code
                            HAVING
                                COUNT(1) > 1
                        ) THEN
                            1
                    END
                ) = 1
        ) wd
        LEFT OUTER JOIN (
            SELECT
                seval.school_code,
                salloc.batch_no# AS batch_no,
                salloc.class_code,
                salloc.term_code,
                salloc.student_id,
                salloc.super_code,
                seval.examination_type
            FROM
                     std_score_alloc salloc
                INNER JOIN school_evaluation seval ON ( seval.school_code = salloc.school_code
                                                        AND seval.evaluation_code = salloc.evaluation_code )
            WHERE
                    salloc.status_name = 'ACTIVE'
                AND seval.examination_type = 'INTERNAL'
            GROUP BY
                seval.school_code,
                salloc.batch_no#,
                salloc.class_code,
                salloc.term_code,
                salloc.student_id,
                salloc.super_code,
                seval.examination_type
        ) z ON ( z.school_code = wd.school_code
                 AND z.batch_no = wd.batch_no
                 AND z.class_code = wd.class_code
                 AND z.term_code = wd.term_code
                 AND z.student_id = wd.student_id
                 AND z.super_code = wd.super_code )
    ORDER BY
        school_code,
        batch_no,
        class_code,
        section_code,
        term_code,
        student_id,
        part_name,
        ranking_type,
        (
            SELECT
                a.serial_no
            FROM
                subject_category a
            WHERE
                a.code = wd.category_code
        ) ASC NULLS LAST,
        (
            SELECT
                a.serial_no
            FROM
                school_domain a
            WHERE
                    a.school_code = wd.school_code
                AND a.class_code = wd.class_code
                AND a.domain_code = wd.domain_code
        ) ASC NULLS LAST,
        (
            SELECT
                a.serial_no
            FROM
                school_super a
            WHERE
                    a.school_code = wd.school_code
                AND a.batch_no# = wd.batch_no
                AND a.class_code = wd.class_code
                AND a.super_code = wd.super_code
        ) ASC NULLS LAST,
        (
            SELECT
                serial_no
            FROM
                school_subject
            WHERE
                    school_code = wd.school_code
                AND batch_no# = wd.batch_no
                AND class_code = wd.class_code
                AND subject_code = wd.subject_code
        ) ASC NULLS LAST
]]></userDefinedSQL>
